{
  "id": "subjective",
  "title": "Subjective",
  "description": "Patient-reported history and presenting symptoms.",
  "fields": [
    {
      "id": "chiefComplaint",
      "type": "text",
      "label": "Chief Complaint",
      "placeholder": "Patient states..."
    },
    {
      "id": "historyProvider",
      "type": "select",
      "label": "History Provided By",
      "options": [
        { "value": "patient", "label": "Patient" },
        { "value": "family", "label": "Family" },
        { "value": "caregiver", "label": "Caregiver" },
        { "value": "bystander", "label": "Bystander" },
        { "value": "lawEnforcement", "label": "Law enforcement" }
      ],
      "placeholder": "Select contributor"
    },
    {
      "id": "symptoms",
      "type": "checkbox-group",
      "label": "Pertinent Symptoms",
      "options": [
        { "value": "sob", "label": "Shortness of breath" },
        { "value": "chestPain", "label": "Chest pain" },
        { "value": "nausea", "label": "Nausea" },
        { "value": "vomiting", "label": "Vomiting" },
        { "value": "weakness", "label": "Weakness" },
        { "value": "dizziness", "label": "Dizziness" }
      ]
    },
    {
      "id": "painScale",
      "type": "number",
      "label": "Pain Scale (0-10)",
      "placeholder": "0-10"
    },
    {
      "id": "notes",
      "type": "textarea",
      "label": "Subjective Notes",
      "rows": 4,
      "helperText": "Summarize SAMPLE history, OPQRST findings, and patient statements."
    },
    {
      "id": "opqrstOnset",
      "type": "text",
      "label": "Onset",
      "placeholder": "When did the symptoms begin?",
      "helperText": "Document the OPQRST onset statement."
    },
    {
      "id": "opqrstProvocation",
      "type": "text",
      "label": "Provocation/Palliation",
      "placeholder": "What makes it better or worse?"
    },
    {
      "id": "opqrstQuality",
      "type": "text",
      "label": "Quality",
      "placeholder": "Describe the symptom quality"
    },
    {
      "id": "opqrstRadiation",
      "type": "text",
      "label": "Radiation",
      "placeholder": "Does the pain travel anywhere?"
    },
    {
      "id": "opqrstSeverity",
      "type": "text",
      "label": "Severity Description",
      "placeholder": "Describe severity beyond the numeric scale"
    },
    {
      "id": "opqrstTimeCourse",
      "type": "text",
      "label": "Time Course",
      "placeholder": "How has the symptom progressed?"
    },
    {
      "id": "sampleAllergies",
      "type": "text",
      "label": "Allergies",
      "placeholder": "e.g. NKDA"
    },
    {
      "id": "sampleMedications",
      "type": "text",
      "label": "Medications",
      "placeholder": "List current medications"
    },
    {
      "id": "samplePastHistory",
      "type": "text",
      "label": "Past Medical History",
      "placeholder": "Pertinent history notes"
    },
    {
      "id": "sampleLastIntake",
      "type": "text",
      "label": "Last Oral Intake",
      "placeholder": "Last intake of food or drink"
    },
    {
      "id": "sampleEvents",
      "type": "text",
      "label": "Events Leading Up",
      "placeholder": "What happened prior to EMS arrival?"
    }
  ]
}
